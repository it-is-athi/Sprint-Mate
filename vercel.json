{
  "buildCommand": "npm run build",
  "outputDirectory": "./frontend/dist",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": null,
  "functions": {
    "backend/server.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/backend/server.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
